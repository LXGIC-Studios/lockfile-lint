  ],
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "@types/node": "^20.0.0"
  },
  "homepage": "https://github.com/LXGIC-Studios/lockfile-lint#readme",
